{
  "name": "breederhq-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "preflight": "node scripts/development/preflight-env.js",
    "dev": "npx dotenv -e .env.dev -- node scripts/development/preflight-env.js && npx dotenv -e .env.dev -- tsx watch src/server.ts",
    "dev:stripe": "echo \"‚ÑπÔ∏è  Starting with Stripe billing enabled (requires STRIPE_SECRET_KEY in .env.dev)\" && npm run dev",
    "build": "node --max-old-space-size=4096 ./node_modules/typescript/lib/tsc.js -p .",
    "start": "node scripts/development/preflight-env.js && node dist/server.js",
    "start:render": "node scripts/boot-with-secrets.js",
    "test": "npx dotenv -e .env.dev -- tsx tests/run-tests.ts",
    "test:contracts": "npx dotenv -e .env.dev -- tsx --test tests/party-api-contracts.test.ts",
    "test:regression": "npx dotenv -e .env.dev -- tsx --test tests/party-migration-regression.test.ts",
    "validate:sql": "npx dotenv -e .env.dev -- tsx scripts/validation/validate-party-migration.ts",
    "prisma:validate": "npx dotenv -e .env.dev.migrate -- prisma validate --schema=prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema=prisma/schema.prisma",
    "db:studio": "npx dotenv -e .env.dev -- prisma studio --schema=prisma/schema.prisma",
    "db:gen": "prisma generate --schema=prisma/schema.prisma",
    "db:pull": "node scripts/development/run-with-env.js .env.dev.migrate prisma db pull --schema=prisma/schema.prisma && node scripts/development/post-db-pull.js",
    "db:pull:prod": "node scripts/development/run-with-env.js .env.prod.migrate prisma db pull --schema=prisma/schema.prisma && node scripts/development/post-db-pull.js",
    "db:new": "npx dbmate --migrations-dir db/migrations new",
    "db:dev:status": "npx dbmate --env-file .env.dev.migrate --migrations-dir db/migrations status",
    "db:dev:up": "node scripts/development/run-with-env.js .env.dev.migrate npx dbmate --migrations-dir db/migrations migrate",
    "db:dev:down": "node scripts/development/run-with-env.js .env.dev.migrate npx dbmate --migrations-dir db/migrations rollback",
    "db:dev:sync": "npm run db:dev:up && npm run db:pull && npm run db:gen",
    "db:dev:dump": "npx dbmate --env-file .env.dev.migrate --migrations-dir db/migrations dump",
    "db:dev:deploy": "node scripts/development/run-with-env.js .env.dev.migrate npx dbmate --migrations-dir db/migrations migrate",
    "db:dev:validate": "node scripts/development/run-with-env.js .env.dev.migrate tsx scripts/migrations/v2/validate-post-import.ts",
    "db:dev:seed:users": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-test-users.ts",
    "db:dev:seed:traits": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-trait-definitions.ts",
    "db:dev:seed:tattooine": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-tattooine-contacts.ts",
    "db:dev:seed:products": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-subscription-products.ts",
    "db:dev:seed:genetics": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-genetics-test-animals.ts",
    "db:dev:seed:titles": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-title-definitions.ts",
    "db:dev:seed:competitions": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-titles-competitions.ts",
    "db:dev:seed:contracts": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-contract-templates.ts",
    "db:dev:seed:supplements": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-supplement-benchmarks.ts",
    "db:dev:seed:microchips": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-microchip-registries.ts",
    "db:dev:seed:rearing": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-rearing-benchmarks.ts",
    "db:dev:seed": "npm run db:dev:seed:traits && npm run db:dev:seed:users && npm run db:dev:seed:products && npm run db:dev:seed:microchips && npm run db:dev:seed:rearing",
    "db:dev:reset": "npm run db:dev:up && npm run db:dev:seed",
    "test:quota": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx scripts/api-testing/test-quota-enforcement.ts",
    "test:usage": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx scripts/api-testing/test-usage-api.ts",
    "test:genetics": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx scripts/api-testing/test-genetics-lab.ts",
    "db:dev:seed:validation": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx scripts/seeding/seed-validation-tenants/seed-dev.ts",
    "db:prod:seed:validation": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx scripts/seeding/seed-validation-tenants/seed-prod.ts",
    "db:prod:seed:traits": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx prisma/seed/seed-trait-definitions.ts",
    "db:prod:seed:contracts": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx prisma/seed/seed-contract-templates.ts",
    "db:prod:seed:supplements": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx prisma/seed/seed-supplement-benchmarks.ts",
    "db:prod:seed:microchips": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx prisma/seed/seed-microchip-registries.ts",
    "db:prod:seed:rearing": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx prisma/seed/seed-rearing-benchmarks.ts",
    "db:prod:status": "npx dbmate --env-file .env.prod.migrate --migrations-dir db/migrations status",
    "db:prod:deploy": "node scripts/development/run-with-env.js .env.prod.migrate npx dbmate --migrations-dir db/migrations migrate",
    "db:prod:up": "node scripts/development/run-with-env.js .env.prod.migrate npx dbmate --migrations-dir db/migrations migrate",
    "db:prod:down": "node scripts/development/run-with-env.js .env.prod.migrate npx dbmate --migrations-dir db/migrations rollback",
    "db:prod:dump": "npx dbmate --env-file .env.prod.migrate --migrations-dir db/migrations dump",
    "bootstrap:admin:dev": "npx dotenv -e .env.dev -- node scripts/admin/bootstrap-global-admin-access.mjs",
    "bootstrap:admin:prod": "npx dotenv -e .env.prod -- node scripts/admin/bootstrap-global-admin-access.mjs",
    "script:enable-public-program": "node scripts/admin/enable-public-program.mjs",
    "script:verify-public-programs": "powershell -File scripts/verification/verify-public-programs.ps1",
    "script:verify-routes": "powershell -File scripts/verification/verify-public-marketplace-routes.ps1",
    "script:remove-quotas:dev": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx scripts/remove-quota-restrictions.ts",
    "script:remove-quotas:prod": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx scripts/remove-quota-restrictions.ts",
    "script:check-quotas:dev": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx scripts/check-quota-status.ts",
    "script:check-quotas:prod": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx scripts/check-quota-status.ts",
    "script:create-unlimited-plan:dev": "node scripts/development/run-with-env.js .env.dev.migrate npx tsx scripts/create-unlimited-plan.ts",
    "script:create-unlimited-plan:prod": "node scripts/development/run-with-env.js .env.prod.migrate npx tsx scripts/create-unlimited-plan.ts",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "db:push": "echo \"üö´ FORBIDDEN - Use: npm run db:dev:up\" && exit 1",
    "db:dev:push": "echo \"üö´ FORBIDDEN - Use: npm run db:dev:up\" && exit 1",
    "db:prod:push": "echo \"üö´ FORBIDDEN - Use: npm run db:prod:deploy\" && exit 1",
    "db:dev:migrate": "echo \"üö´ Prisma Migrate is REMOVED. Use: npm run db:new <name> then npm run db:dev:sync\" && exit 1",
    "db:dev:baseline": "echo \"üö´ Prisma Migrate is REMOVED. Baseline is managed by dbmate.\" && exit 1",
    "db:prod:baseline": "echo \"üö´ Prisma Migrate is REMOVED. Baseline is managed by dbmate.\" && exit 1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/client-secrets-manager": "^3.981.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.4.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/redis": "^7.2.0",
    "@fastify/websocket": "^11.2.0",
    "@prisma/client": "6.17.1",
    "@sentry/node": "^10.38.0",
    "@sentry/profiling-node": "^10.38.0",
    "@simplewebauthn/server": "^13.2.2",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "fastify": "^5.7.3",
    "firebase-admin": "^13.6.1",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "otplib": "^13.1.0",
    "pdf-lib": "^1.17.1",
    "resend": "^6.6.0",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "stripe": "^20.1.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/ws": "^8.18.1",
    "cross-env": "^10.1.0",
    "dbmate": "^2.30.0",
    "dotenv-cli": "^7.4.4",
    "prisma": "6.17.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
