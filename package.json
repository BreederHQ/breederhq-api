{
  "name": "breederhq-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "preflight": "node scripts/preflight-env.js",
    "dev": "npx dotenv -e .env.dev -- node scripts/preflight-env.js && npx dotenv -e .env.dev -- tsx watch src/server.ts",
    "dev:stripe": "echo \"‚ÑπÔ∏è  Starting with Stripe billing enabled (requires STRIPE_SECRET_KEY in .env.dev)\" && npm run dev",
    "build": "tsc -p .",
    "start": "node scripts/preflight-env.js && node dist/server.js",
    "start:render": "node scripts/preflight-env.js && node dist/server.js",
    "test": "npx dotenv -e .env.dev -- tsx tests/run-tests.ts",
    "test:contracts": "npx dotenv -e .env.dev -- tsx --test tests/party-api-contracts.test.ts",
    "test:regression": "npx dotenv -e .env.dev -- tsx --test tests/party-migration-regression.test.ts",
    "validate:sql": "npx dotenv -e .env.dev -- tsx scripts/validate-party-migration.ts",
    "prisma:validate": "npx dotenv -e .env.dev.migrate -- prisma validate --schema=prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema=prisma/schema.prisma",
    "db:studio": "npx dotenv -e .env.dev -- prisma studio --schema=prisma/schema.prisma",
    "db:gen": "prisma generate --schema=prisma/schema.prisma",
    "db:dev:status": "node scripts/run-with-env.js .env.dev.migrate prisma migrate status --schema=prisma/schema.prisma",
    "db:dev:migrate": "node scripts/run-with-env.js .env.dev.migrate prisma migrate dev --schema=prisma/schema.prisma",
    "db:dev:validate": "node scripts/run-with-env.js .env.dev.migrate tsx scripts/db/v2/validate-post-import.ts",
    "db:dev:validate:schema": "node scripts/run-with-env.js .env.dev.migrate node scripts/db/v2/validate-schema-drift.js",
    "db:dev:baseline": "node scripts/run-with-env.js .env.dev.migrate prisma migrate resolve --applied 20251230112400_init --schema=prisma/schema.prisma",
    "db:dev:deploy": "node scripts/run-with-env.js .env.dev.migrate prisma migrate deploy --schema=prisma/schema.prisma",
    "db:dev:integrity": "node scripts/run-with-env.js .env.dev.migrate node scripts/db/check-migration-integrity.mjs",
    "db:validate:order": "node scripts/db/validate-migration-order.mjs",
    "db:dev:seed:users": "node scripts/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-test-users.ts",
    "db:dev:seed:traits": "node scripts/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-trait-definitions.ts",
    "db:dev:seed:tattooine": "node scripts/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-tattooine-contacts.ts",
    "db:dev:seed:products": "node scripts/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-subscription-products.ts",
    "db:dev:seed:genetics": "node scripts/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-genetics-test-animals.ts",
    "db:dev:seed:titles": "node scripts/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-title-definitions.ts",
    "db:dev:seed:competitions": "node scripts/run-with-env.js .env.dev.migrate npx tsx prisma/seed/seed-titles-competitions.ts",
    "db:dev:seed": "npm run db:dev:seed:traits && npm run db:dev:seed:users && npm run db:dev:seed:products",
    "db:dev:reset": "npm run db:dev:deploy && npm run db:dev:seed",
    "test:quota": "node scripts/run-with-env.js .env.dev.migrate npx tsx scripts/test-quota-enforcement.ts",
    "test:usage": "node scripts/run-with-env.js .env.dev.migrate npx tsx scripts/test-usage-api.ts",
    "test:genetics": "node scripts/run-with-env.js .env.dev.migrate npx tsx scripts/test-genetics-lab.ts",
    "db:prod:seed:traits": "node scripts/run-with-env.js .env.prod.migrate npx tsx prisma/seed/seed-trait-definitions.ts",
    "db:prod:status": "node scripts/run-with-env.js .env.prod.migrate prisma migrate status --schema=prisma/schema.prisma",
    "db:prod:deploy": "node scripts/run-with-env.js .env.prod.migrate prisma migrate deploy --schema=prisma/schema.prisma",
    "db:prod:validate": "node scripts/run-with-env.js .env.prod.migrate tsx scripts/db/v2/validate-post-import.ts",
    "db:prod:validate:schema": "node scripts/run-with-env.js .env.prod.migrate node scripts/db/v2/validate-schema-drift.js",
    "db:prod:baseline": "node scripts/run-with-env.js .env.prod.migrate prisma migrate resolve --applied 20251230112400_init --schema=prisma/schema.prisma",
    "db:prod:integrity": "node scripts/run-with-env.js .env.prod.migrate node scripts/db/check-migration-integrity.mjs",
    "db:dev:grant": "node scripts/db/v2/run-grant-permissions.js dev",
    "db:prod:grant": "node scripts/db/v2/run-grant-permissions.js prod",
    "db:print-target": "node scripts/print-db-target.js",
    "db:move:dev:preflight": "node scripts/db/v2/preflight-dev-move.js",
    "db:move:dev:run": "node scripts/db/v2/dev-move-runbook.js",
    "db:move:dev:dump": "node scripts/db/v2/run-dump.js dev",
    "db:move:dev:import": "node scripts/db/v2/run-import.js dev",
    "db:move:dev:postimport": "node scripts/db/v2/run-postimport.js dev",
    "db:move:prod:preflight": "node scripts/db/v2/preflight-prod-move.js",
    "db:move:prod:run": "node scripts/db/v2/prod-move-runbook.js",
    "db:move:prod:dump": "node scripts/db/v2/run-dump.js prod",
    "db:move:prod:import": "node scripts/db/v2/run-import.js prod",
    "db:move:prod:postimport": "node scripts/db/v2/run-postimport.js prod",
    "bootstrap:admin:dev": "npx dotenv -e .env.dev -- node scripts/bootstrap-global-admin-access.mjs",
    "bootstrap:admin:prod": "npx dotenv -e .env.prod -- node scripts/bootstrap-global-admin-access.mjs",
    "script:enable-public-program": "node scripts/enable-public-program.mjs",
    "script:verify-public-programs": "powershell -File scripts/verify-public-programs.ps1",
    "script:verify-routes": "powershell -File scripts/verify-public-marketplace-routes.ps1",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "db:push": "echo \"üö´ db push is FORBIDDEN - Use: npm run db:dev:migrate\" && exit 1",
    "db:dev:push": "echo \"üö´ db push is FORBIDDEN - Use: npm run db:dev:migrate\" && exit 1",
    "db:prod:push": "echo \"üö´ db push is FORBIDDEN - Use: npm run db:prod:deploy\" && exit 1"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@prisma/client": "^6.17.1",
    "bcryptjs": "^3.0.2",
    "fastify": "^5.6.0",
    "resend": "^6.6.0",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6",
    "stripe": "^20.1.2",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^7.4.4",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
